{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "eslint-config-upleveled/tsconfig.base.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "jsx": "preserve"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "**/*.cjs",
    "**/*.mjs",
    ".next/types/**/*.ts",
    ".next/types/app/products/[products]/page.js",
    ".next/types/app/page.js",
    ".next/types/app/layout.js",
    ".next/types/app/products/page.js",
    ".next/types/app/contact/page.js",
    ".next/types/app/about/page.js"
  ]
}
